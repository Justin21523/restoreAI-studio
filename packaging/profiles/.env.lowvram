# Low VRAM optimization profile
DEVICE=cuda
USE_FP16=true
TILE_SIZE=256
MAX_WORKERS=2
JOBS_MAX_WORKERS=2

# Performance settings
FORCE_FP16=true
FORCE_TILING=true
ATTENTION_SLICING=true
ALLOW_OFFLOAD=true

# Resource limits
MAX_LOADED_MODELS=2
IDLE_UNLOAD_SEC=180
MIN_FREE_GB=1.0

# Video processing
VIDEO_MAX_SECONDS=15
CHUNK_FRAMES=60
OVERLAP_FRAMES=1